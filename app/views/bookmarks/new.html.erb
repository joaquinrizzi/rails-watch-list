<div class="container mt-3">
  <div class="row">
    <div class="col-sm-8 offset-sm-2">
      <h1>New Bookmark</h1>
      <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.input :comment %>
        <%= f.association :movie %>
        <%= f.button :submit, class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
